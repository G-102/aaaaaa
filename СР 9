import re

def найти_минимальный_год(имя_файла):
    """
    Находит год с наименьшим номером в файле, содержащем даты.

    Args:
        имя_файла: Имя файла, содержащего даты в формате число месяц год.
                    Например: "15 января 2023" или "20 02 2024".

    Returns:
        Минимальный год (целое число) или None, если файл пустой или не содержит дат.
    """

    минимальный_год = None

    try:
        with open(имя_файла, 'r', encoding='utf-8') as файл:
            for строка in файл:
                match = re.search(r'\d{4}', строка)  

                if match:
                    год = int(match.group(0))  

                    if минимальный_год is None or год < минимальный_год:
                        минимальный_год = год

    except FileNotFoundError:
        print(f"Ошибка: Файл '{имя_файла}' не найден.")
        return None
    except Exception as e:
        print(f"Произошла ошибка при чтении файла: {e}")
        return None

    return минимальный_год
